<UserControl x:Class="MVVM.Views.ViewC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:prism="http://prismlibrary.com/"             
             prism:ViewModelLocator.AutoWireViewModel="True">
    <Grid>
        <StackPanel>
            <Label Content="ViewC画面"
                   FontSize="30"
                   Foreground="Blue"/>
            <TextBox Text="{Binding ViewCTextBox}"/>
            <Button Content="OK" Command="{Binding OKButton}"/>
        </StackPanel>
    </Grid>
</UserControl>
